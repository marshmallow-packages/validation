{
  "name": "marshmallow/validation",
  "description": "Additional validation rules for the Laravel framework",
  "keywords": [
    "validation",
    "Validator",
    "laravel",
    "bic",
    "iban",
    "creditcard",
    "isbn",
    "ean",
    "gtin",
    "isin",
    "luhn",
    "base64",
    "data-url",
    "ulid"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "ext-mbstring": "*",
    "illuminate/validation": "^10|^11"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "phpstan/phpstan": "^1",
    "symfony/uid": "^5.1|^6.2",
    "slevomat/coding-standard": "~8.0",
    "squizlabs/php_codesniffer": "^3.8"
  },
  "autoload": {
    "psr-4": {
      "Marshmallow\\Validation\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Marshmallow\\Validation\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Marshmallow\\Validation\\Laravel\\ValidationServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
